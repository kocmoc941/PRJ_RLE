function uPreroll_setcookie(){
	var s = document.createElement('script');
	s.src="http://s3.ucoz.net/cgi/uutils.fcg?a=get_preroll_cookie&aa=1&r=0.192374911034456" ;
	document.head.appendChild(s);
}

