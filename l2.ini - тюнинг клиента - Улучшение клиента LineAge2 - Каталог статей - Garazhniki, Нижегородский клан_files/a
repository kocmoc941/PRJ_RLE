		dummy_video : function(containerid){
			var rnd = Math.random() ;
			for(key in uPreroll.dummy) {
				if(rnd < uPreroll.dummy[key].rate) {
					uPreroll.dummy[key].show(containerid);
					break ;
				} else {
					rnd -= uPreroll.dummy[key].rate ;
				}
			}
